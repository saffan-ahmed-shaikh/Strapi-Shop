{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string"
    },
    "Amount": {
      "type": "integer"
    },
    "Image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "ProductId": {
      "type": "uid"
    },
    "description": {
      "type": "text"
    },
    "Count": {
      "type": "enumeration",
      "enum": [
        "one",
        "two",
        "three",
        "four",
        "more than four."
      ]
    }
  }
}
